<?xml version="1.0" encoding="UTF-8" ?>

<beans 
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
		http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-3.0.xsd">

    <bean id="applicationService" class="org.esupportail.sifacmissions.services.application.ApplicationServiceImpl">
    	<property name="name" value="esup-sifacmiss2"/>
    	<property name="versionString" value="${app.version}"/>
    	<property name="copyright" value="Copyright (c) 2012 ESUP-Portail consortium" />
    	<property name="vendor" value="ESUP-Portail consortium" />
    	<property name="latestVersionBaseUrl" value="http://sourcesup.cru.fr/esup-sifacmiss2" />
    </bean>

</beans>
