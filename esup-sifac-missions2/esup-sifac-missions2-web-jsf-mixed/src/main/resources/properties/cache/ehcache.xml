<ehcache>

	<diskStore path="${cache.path}" />

	<defaultCache
		maxElementsInMemory="10000" timeToIdleSeconds="120" timeToLiveSeconds="120"
		memoryStoreEvictionPolicy="LRU" eternal="false" overflowToDisk="true"
		diskPersistent="false" diskExpiryThreadIntervalSeconds="120" />

	<cache name="org.esupportail.commons.services.exceptionHandling.CachingEmailExceptionServiceImpl"
		maxElementsInMemory="1000" timeToIdleSeconds="900" timeToLiveSeconds="900"
		eternal="false" overflowToDisk="true" />

	<cache name="org.esupportail.commons.services.ldap.CachingLdapServiceImpl"
		maxElementsInMemory="1000" timeToIdleSeconds="300" timeToLiveSeconds="600"
		eternal="false" overflowToDisk="true" />

	<cache name="User"
		maxElementsInMemory="1000" timeToIdleSeconds="900" timeToLiveSeconds="1800"
		eternal="false" overflowToDisk="true" />

	<cache name="ListOfFraisMission"
		maxElementsInMemory="1000" timeToIdleSeconds="450" timeToLiveSeconds="900"
		eternal="false" overflowToDisk="true" />

</ehcache>
