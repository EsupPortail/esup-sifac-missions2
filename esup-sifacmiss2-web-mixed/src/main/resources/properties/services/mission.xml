<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="sifacMissionService"
		class="org.esupportail.sifacmissions.services.sifac.SifacMissionService">
		<property name="mandant" value="${sifac.mandant}" />
		<property name="firstYear" value="${sifac.firstYear}" />
		<property name="portailService" ref="sifacPortail" />
		<property name="cacheManager" ref="cacheManager" />
		<property name="cacheName" value="ListOfFraisMission" />
	</bean>

</beans>
